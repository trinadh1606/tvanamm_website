{
  "rewrites": [
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },

    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(googlebot|bingbot|baiduspider|yandex|duckduckbot|slurp|semrush|ahrefs|facebookexternalhit|twitterbot).*"
        }
      ],
      "destination": "https://service.prerender.io/https://tvanamm.com/:path*?prerenderToken=8S7qE3sySwmostB8DLZJ"
    },

    { "source": "/:path*", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
